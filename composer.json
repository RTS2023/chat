{
    "name": "iulian/chat",
    "description": "Messenger functionality for Laravel 9.0",
    "authors": [
        {
            "name": "iulian",
            "email": "onofrei.iulian1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Messenger\\": "src/Messenger/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Messenger\\MessengerServiceProvider"
            ],
            "aliases": {
                "Messenger": "Messenger\\Facades\\Messenger"
            }
        }
    }
}
